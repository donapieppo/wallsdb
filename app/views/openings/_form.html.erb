<% @wall.openings.each do |opening| %>  
  <%= opening %> <%= link_to_delete opening_path(opening) %> <br/> 
<% end %>

<% @opening = @wall.openings.new %>  

<%= simple_form_for [@wall, @opening] do |opening| %>  
  <%= select_opening_day %>
  dalle <%= select_opening_hour(:open) %> alle <%= select_opening_hour(:close) %>  
  <%= opening.submit %>  
<% end %>
