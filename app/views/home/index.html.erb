<div class="home-container">
<div class="home-contents"> 
  <label>Scegli la città</label>
  <input class="typeahead" type="text" placeholder="Città">  
  <input type="submit" value="invia"/>
  <h2>Prossimi Eventi</h2>
  dsdds
  <h2>Ultime Palestre Inserite</h2>
  dsd
</div>

<div class="home-map">
  <div id="map"></div>
</div>
</div><!-- home-container -->

<script>
  var rome  = [41.9102415,12.3959142,11];
  var walls = <%= raw Wall.geocodes.to_json %>;
  var map;
  function initMap() {
    map = new google.maps.Map(document.getElementById('map'), {
          zoom: 6,
          center: {lat: rome[0], lng: rome[1]}
    });
    walls.forEach(function(item, index) {
      new google.maps.Marker({ position: {lat: item[0], lng: item[1]}, map: map, title: item[2] });
    });
  }
</script>
<script async defer
  src="https://maps.googleapis.com/maps/api/js?key=<%= ENV['WALLSDB_GOOGLE_MAP_API_KEY'] %>&signed_in=true&callback=initMap&libraries=places">
</script>

